#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ipr/dev/Basic programs/routes/node_modules/csvtojson/bin/node_modules:/home/ipr/dev/Basic programs/routes/node_modules/csvtojson/node_modules:/home/ipr/dev/Basic programs/routes/node_modules:/home/ipr/dev/Basic programs/node_modules:/home/ipr/dev/node_modules:/home/ipr/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ipr/dev/Basic programs/routes/node_modules/csvtojson/bin/node_modules:/home/ipr/dev/Basic programs/routes/node_modules/csvtojson/node_modules:/home/ipr/dev/Basic programs/routes/node_modules:/home/ipr/dev/Basic programs/node_modules:/home/ipr/dev/node_modules:/home/ipr/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../csvtojson/bin/csvtojson" "$@"
else
  exec node  "$basedir/../csvtojson/bin/csvtojson" "$@"
fi
